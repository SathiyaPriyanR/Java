
package com.mycompany.sumodds;

public class Sumodds {

   
   public static void main(String[] args) {
      int i = 326749;
      int sum = 0;

      while (i != 0) {
         int digit = i % 10;
         if (digit % 2 != 0) {
            sum += digit;
         }
         i /= 10;
      }

      System.out.println("The sum of the odd digits is: " + sum);
   }
}

