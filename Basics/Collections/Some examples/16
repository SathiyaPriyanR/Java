
import java.util.ArrayList;
public class Sample17 {
    public static void main(String[] args) {
        ArrayList <String> a = new ArrayList<>();
        a.add("Redmi");
        a.add("Oppo");
        a.add("Vivo");
        a.add("Samsung");
        a.clear();
        System.out.println(a);   // Empty the list
    }
}
